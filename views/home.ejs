          <h1>Blog</h1>
          <h4>This section is focused on live tweets updates from FreeCodeCamp. It is to highlight what I use to stay updated on trends and new tutorials regarding my favourite programming languages.</h4>
        </div>
          <div class="card-wrap content">
          <%data.forEach(function(item){ %>
              <div class="card alignCenter">
                <article role="article">
                  <% let entity = item.entities%>
                  <%if(entity === undefined){%>
                    <p><%=item%> </p>
                  <%} else {%>
                  <% let url =entity.urls%>
                  
                  <%url.forEach(function(entit){ %>
                    <h3>@FreeCodeCamp</h3>
                    <p> <%=(item.text.substr(0, entit.start)).replace(/&amp;/g, "&") %></p>

                      <a target="_blank" rel="noreferrer" href="<%=entit.url%>"> <%=entit.display_url%></a>
                  
                      <%})%>
                  <%}%>
                </article>
              </div>

          <%})%>
          <a class="prev" >&#10094; </a>
          <a class="next" >&#10095;</a>
          </div>

      </div>
